
# vim:ft=sshconfig

CanonicalizeHostname yes
CanonicalDomains m45.me almg.uucp almg.gov.br local
CanonicalizeMaxDots 0
CanonicalizeFallbackLocal yes

Match originalhost "e????,sisap*,sweb*" exec "~/.usr/bin/AlmgVPN"
        ForwardX11 yes
        ForwardX11Trusted yes
        IdentityFile ~/.ssh/almg/id_rsa

Match originalhost oo exec "~/.usr/bin/NotConstitution"
        HostName constitution

Match originalhost oo
        HostName mississipi

#
# Defaults
#

Host *
        Compression yes
        ServerAliveInterval 5
        ServerAliveCountMax 2

Host *.local
        ForwardX11 yes
        ForwardX11Trusted yes

Host *.almg.uucp
        ForwardX11 yes
        ForwardX11Trusted yes
        IdentityFile ~/.ssh/almg/id_rsa

Host m45.me me
        HostName m45.me
        User ubuntu
        IdentityFile ~/.ssh/id_aws.pem

Host e8099
        RequestTTY auto

Host almgsocks exec "~/.usr/bin/AlmgVPN"
        HostName e8099
        RequestTTY no
        DynamicForward 0.0.0.0:1080
        LocalForward 59000 e4002218:5900
        LocalForward 51521 172.18.10.34:1521

Host sisap-backend sweb
        HostName sisap-backend
        User ggov
        ForwardX11 no
        PasswordAuthentication no
        IdentityFile ~/.ssh/almg/id_rsa
        ProxyJump e8099

Host sisap sisaph-backend swebh
        HostName sisap
        User ggov
        ForwardX11 no
        PasswordAuthentication no
        IdentityFile ~/.ssh/almg/id_rsa
        ProxyJump e8099

Host gitlab gitlab.almg.uucp
        HostName gitlab
        User git
        ForwardX11 no
        PasswordAuthentication no
        IdentityFile ~/.ssh/almg/id_rsa
        ProxyJump e8099

Host admap-p exec "~/.usr/bin/AlmgVPN"
        HostName sisap-backend
        User ggov
        ForwardX11 no
        PasswordAuthentication no
        IdentityFile ~/.ssh/almg/id_rsa
        ProxyJump e8099
        RequestTTY no
        LocalForward 33302 localhost:3306

Host admap-h exec "~/.usr/bin/AlmgVPN"
        HostName sisap
        User ggov
        ForwardX11 no
        PasswordAuthentication no
        IdentityFile ~/.ssh/almg/id_rsa
        ProxyJump e8099
        RequestTTY no
        LocalForward 33301 localhost:3306

Host listenvnc exec "~/.usr/bin/AlmgVPN"
        HostName e8099
        RequestTTY no
        RemoteForward 0.0.0.0:5500 localhost:5500

Match originalhost jumpy
        HostName localhost
        Port 8099
        User h
        ProxyJump me

Host github github.com
        HostName github.com
        User git
        ForwardX11 no
        PasswordAuthentication no
        IdentityFile ~/.ssh/id_rsa
        RequestTTY no

Host afal gitlab.afal.com.br
        HostName gitlab.afal.com.br
        User git
        ForwardX11 no
        PasswordAuthentication no
        IdentityFile ~/.ssh/id_rsa
        RequestTTY no



